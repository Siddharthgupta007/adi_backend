{
  "kind": "collectionType",
  "collectionName": "kids_products",
  "info": {
    "singularName": "kids-product",
    "pluralName": "kids-products",
    "displayName": "Kids-product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Image": {
      "allowedTypes": [
        "images",
        "videos"
      ],
      "type": "media",
      "multiple": false
    },
    "Price": {
      "type": "integer"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Subcategory": {
      "type": "enumeration",
      "enum": [
        "sneaker",
        "sportshoe",
        "slippers",
        "shirt",
        "tshirt",
        "pant",
        "caps",
        "swiming",
        "sandals",
        "hiking"
      ]
    },
    "Colour": {
      "type": "enumeration",
      "enum": [
        "White",
        "Black",
        "Orange\t",
        "Maroon",
        "Red\tYellow\t",
        "Lime green\t",
        "Green\t",
        "Sky blue\t",
        "Grey",
        "Violet",
        "Brown",
        "Navy Blue",
        "Amber",
        "Ash\tJet black"
      ]
    },
    "Description": {
      "type": "text"
    },
    "New_drop": {
      "type": "boolean"
    },
    "Instock": {
      "type": "boolean",
      "default": true
    },
    "Quantity": {
      "type": "integer"
    },
    "Size": {
      "type": "json"
    }
  }
}
